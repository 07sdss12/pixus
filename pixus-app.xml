<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<application xmlns="http://ns.adobe.com/air/application/1.5">

  <id>com.codeplay.pixus</id>

  <version>1.2</version>

  <filename>Pixus</filename>

  <description>The pixel ruler for Adobe AIR. A handy tool for web and interface designers. (Runs on Windows / Mac OS X)</description>

  <name>Pixus</name>

  <copyright>(cc) 2007-2010 JPEG Interactive</copyright>

  <initialWindow>
    <content>pixus.swf</content>
    <systemChrome>none</systemChrome>
    <transparent>true</transparent>
    <visible>false</visible>
    <maximizable>false</maximizable>
    <minimizable>false</minimizable>
    <resizable>false</resizable>
    
    
    
    
    
    
    
    
  </initialWindow>

  <programMenuFolder>JPEG Interactive</programMenuFolder>

  <icon>
    <image128x128>icons/AIRApp_128.png</image128x128>
    <image48x48>icons/AIRApp_48.png</image48x48>
    <image32x32>icons/AIRApp_32.png</image32x32>
    <image16x16>icons/AIRApp_16.png</image16x16>
  </icon>

  <customUpdateUI>false</customUpdateUI>

  <allowBrowserInvocation>false</allowBrowserInvocation>

  <installFolder>jpeg</installFolder>




</application>
